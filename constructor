class B {
    String name;
    int num;
    B() {
        name = "CSE";
        num = 5;
    }
    B(String str,int n) {
        name = str;
        num = n;
    }
    public static void main(String []args) {
        B b1 = new B();
        B b2 = new B("Ritu",1);
        System.out.println(b2.name);
        System.out.println(b2.num); 
        System.out.println(b1.name);  
        System.out.println(b1.num);
    }
}
