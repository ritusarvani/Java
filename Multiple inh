interface A {
    void showA ();
}
interface B {
    void showB ();
}
interface C {
    void showC ();
}
class D implements A,B,C {
    public static void main(String []args) {
        D d = new D();
        d.showA();
        d.showB();
        d.showC();
    }
    public void showA() {
        System.out.println("Interface A");
    }
    public void showB() {
        System.out.println("Interface B");
    }
    public void showC() {
        System.out.println("Interface C");
    }
}
    
