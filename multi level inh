class A {
    public void methodA() {
        System.out.println("Grand parent");
    }
}
class B extends A {
    public void methodB() {
        System.out.println("Parent");
    }
}
class C extends B {
     public void methodC() {
        System.out.println("Child");
    }
    public static void main(String []args) {
        C c = new C();
        c.methodA();
        c.methodB();
        c.methodC();
    }
}
